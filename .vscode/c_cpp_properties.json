{
    "configurations": [
        {
            "name": "ESP-IDF",
            "compilerPath": "${config:idf.toolsPath}/tools/xtensa-esp-elf/esp-13.2.0_20230928/xtensa-esp-elf/bin/xtensa-esp-elf-gcc",
            "compileCommands": "${workspaceFolder}/example1_bt/a2dp_sink/build/compile_commands.json",
            "includePath": [
                "${config:idf.espIdfPath}/components/**",
                "${config:idf.espIdfPathWin}/components/**",
                "${workspaceFolder}/**",
                "${config:idf.espIdfPath}/components/driver/include",
                "${config:idf.espIdfPath}/components/newlib/include",
                "${config:idf.espIdfPath}/components/freertos/FreeRTOS-Kernel/include",
                "${config:idf.espIdfPath}/components/log/include",
                "${config:idf.espIdfPath}/components/esp_common/include",
                "${config:idf.espIdfPath}/components/esp_system/include"
            ],
            "browse": {
                "path": [
                    "${config:idf.espIdfPath}/components",
                    "${config:idf.espIdfPathWin}/components",
                    "${workspaceFolder}"
                ],
                "limitSymbolsToIncludedHeaders": true
            },
            "defines": [
                "CONFIG_LOG_MAXIMUM_LEVEL=3",
                "ESP_LOG_DEFAULT_LEVEL=3",
                "_lock_t=int",
                "CONFIG_EXAMPLE_I2S_BCK_PIN=26",
                "CONFIG_EXAMPLE_I2S_LRCK_PIN=22",
                "CONFIG_EXAMPLE_I2S_DATA_PIN=25",
                "I2S_CLK_SRC_DEFAULT=0"
            ],
            "cStandard": "c11",
            "intelliSenseMode": "gcc-x64"
        }
    ],
    "version": 4
}
